  /// 高德地图 API Key
  /// 申请地址：https://lbs.amap.com/
  /// 免费额度：30万次/天（个人开发者）
  static const String amap = 'YOUR_AMAP_API_KEY_HERE';

  /// 百度翻译 API Key
  /// 申请地址：https://fanyi-api.baidu.com/
  /// 免费额度：5万字符/月
  static const String baiduTranslateAppId = 'YOUR_BAIDU_APPID_HERE';
  static const String baiduTranslateKey = 'YOUR_BAIDU_KEY_HERE';
}
